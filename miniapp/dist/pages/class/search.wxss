
.view-main {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 50rpx;
}
.view-search {
  width: 92%;
  height: 2.5rem;
  background-color: #f5f6f8;
  border-radius: 20px;
}
.border-input {
  width: 100%;
  height: 100%;
  margin-left: 30rpx;
}
.view-history {
  width: 100%;
  display: flex;
  align-items: center;
  flex-direction: column;
}
.view-class {
  display: flex;
  flex-wrap: wrap;
  width: 92%;
}
.view-wrap {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 1.9rem;
  border-radius: 20px;
  background-color: #f5f6f8;
  margin-right: 20rpx;
  margin-bottom: 20rpx;
}
.text-class {
  font-size: 12px;
  margin: 30rpx;
}
