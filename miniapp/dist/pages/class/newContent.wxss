
.main {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.view-text {
  display: flex;
  justify-content: center;
  width: 95%;
  padding-top: 10rpx;
  padding-bottom: 30rpx;
}
.view-all-photo{
  display: flex;
  flex-wrap: wrap;
  height: 300rpx;
  width: 95%;
}
.view-photo {
  display: flex;
  justify-content: center;
  align-items: center; 
  width: 170rpx;
  height: 170rpx;
}
.photo {
  width: 150rpx;
  height: 150rpx;
}
.view-button {
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  border-top: 1px solid lightgrey;
}
.button {
  margin-top: 30rpx;
  background-color: #32CD32;
  font-size: 25rpx;
  color: white;
  width: 150rpx;
}
